<h1>Subtask Question Users
  <%= link_to new_subtask_question_user_path, class: 'btn btn-success' do %>
    <i class="glyphicon glyphicon-plus"></i> New Subtask Question User
  <% end %>
</h1>

<table class="table">
  <thead>
    <tr>
      <th>Subtask question</th>
      <th>User</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @subtask_question_users.each do |subtask_question_user| %>
      <tr>
        <td><%= subtask_question_user.subtask_question_id %></td>
        <td><%= subtask_question_user.user_id %></td>
        <td><%= subtask_question_user.description %></td>
        <td><%= link_to 'Show', subtask_question_user, class: 'btn btn-primary btn-xs'   %></td>
        <td><%= link_to 'Edit', edit_subtask_question_user_path(subtask_question_user), class: 'btn btn-info btn-xs'   %></td>
        <td><%= link_to 'Destroy', subtask_question_user, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-xs'   %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Subtask question user', new_subtask_question_user_path %>
